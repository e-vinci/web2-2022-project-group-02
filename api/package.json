{
  "name": "web2-2022-project-group-02-backend",
  "version": "1.0.0",
  "description": "CatByte Backend",
  "type": "module",
  "scripts": {
    "prepare": "cd .. && husky install api/.husky",
    "debug": "npm run dev",
    "dev": "nodemon ./bin/www.js",
    "start": "node ./bin/www.js",
    "lint": "eslint **/*.js",
    "lint:fix": "npm run lint -- --fix"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*"
    ],
    "exec": "npm run lint && node"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "comlink": "^4.3.1",
    "cookie-parser": "~1.4.4",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "indexeddbshim": "^10.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3"
  }
}
